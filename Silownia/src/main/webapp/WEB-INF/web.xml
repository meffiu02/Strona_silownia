<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Silownia</display-name>
  <display-name>jedzenie</display-name>
  <servlet>  
<servlet-name>RegistrationServlet2</servlet-name>  
<servlet-class>RegistrationServlet2</servlet-class>  
</servlet>  
  
<servlet-mapping>  
<servlet-name>RegistrationServlet2</servlet-name>  
<url-pattern>/com.uniquedeveloper.registration/RegistrationServlet2</url-pattern>  
</servlet-mapping>  
  

  
  <welcome-file-list>
    <welcome-file>main.jsp</welcome-file>
    
  </welcome-file-list>

<filter>
    <filter-name>encodingFilter</filter-name>
    <filter-class>org.apache.catalina.filters.SetCharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>encodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>


</web-app>